<p id="notice"><%= notice %></p>

<h1>Urls</h1>

<div>
  <%= link_to 'New Url', new_url_path %>
</div>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @urls.each do |url| %>
      <tr>
        <td><%= link_to url.url, u_url(url.token), target: '_blank' %></td>
      </tr>
    <% end %>
  </tbody>
</table>
